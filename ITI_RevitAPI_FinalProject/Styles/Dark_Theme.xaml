<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Color_Border">#FF0000</Color>
    <Color x:Key="Color_Background_Primary">#040F24</Color>
    <Color x:Key="Color_Background_Secondary">#05132B</Color>
    <Color x:Key="Color_BackgroundDisabled">#222224</Color>
    <Color x:Key="Color_Text_Primary">#F7F7F7</Color>
    <Color x:Key="Color_Text_Secondary">#E6E6E6</Color>
    <Color x:Key="Color_Accent1">#1E00FF</Color>
    <Color x:Key="Color_Accent2">#FF0000</Color>

    <SolidColorBrush x:Key="Brush_Border" Color="{StaticResource Color_Border}"/>
    <SolidColorBrush x:Key="Brush_Background_Primary" Color="{StaticResource Color_Background_Primary}"/>
    <SolidColorBrush x:Key="Brush_Background_Secondary" Color="{StaticResource Color_Background_Secondary}"/>
    <SolidColorBrush x:Key="Brush_BackgroundDisabled" Color="{StaticResource Color_BackgroundDisabled}"/>
    <SolidColorBrush x:Key="Brush_Text_Primary" Color="{StaticResource Color_Text_Primary}"/>
    <SolidColorBrush x:Key="Brush_Text_Secondary" Color="{StaticResource Color_Text_Secondary}"/>
    <SolidColorBrush x:Key="Brush_Accent1" Color="{StaticResource Color_Accent1}"/>
    <SolidColorBrush x:Key="Brush_Accent2" Color="{StaticResource Color_Accent2}"/>
    
    
</ResourceDictionary>